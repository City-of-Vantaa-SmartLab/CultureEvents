{
  "extends": ["tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  },
  "rules": {
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any",
        "named-imports-order": "any"
      }
    ],
    "jsx-no-lambda": false,
    "interface-name": false,
    "jsx-boolean-value": false,
    "no-console": false
  }
}
